    		System.debug(accountList1);

		}
		system.debug(accountList.size());
	}

	public void question1_2()
	{
		system.debug('We are in question 1_2');
		List<Account> accountList =[SELECT AccountNumber,Name FROM Account];	
		for (Integer count=0; count<accountList.size(); count++)
		 {

    		System.debug(accountList1);

		}
		
	}
	
	


}