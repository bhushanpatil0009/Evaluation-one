public with sharing class AdvancedConcept {
	
	
	public void accountRecord()
	{
		List<Account> accountList =[SELECT AccountNumber,Name FROM Account];
		// Create account sObjects
		//Account object1 ;
		Account a1 = new Account(Name='Account3');
		Account a2 = new Account(Name='Account4');
		// Add accounts to the list
		accountList.add(a1);
		accountList.add(a2);
		// Bulk insert the list
		insert accountList;
		System.debug('Value Of Accounts'+accountList);	
	}


}